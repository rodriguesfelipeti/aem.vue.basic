<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Properties"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
        <items jcr:primaryType="nt:unstructured">
            <column
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/container">
                <items jcr:primaryType="nt:unstructured">
                    <info
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldLabel="Informação"
                        name="./info"/>
                    <txtSearch
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldLabel="Texto Busca"
                        name="./txtSearch"/>
                    <refreshText
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldLabel="Título Refresh Tela"
                        name="./refreshText"/>
                    <refreshCountNumber
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldLabel="Tempo de logout"
                        name="./refreshCountNumber"/>
                    <refreshCountText
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldLabel="Texto Tempo Logout"
                        name="./refreshCountText"/>
                    <continueText
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldLabel="Texto Botão 1"
                        name="./continueText"/>
                    <logoutText
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldLabel="Texto Botão 2"
                        name="./logoutText"/>
                    <urlRouter
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="cq/gui/components/coral/common/form/pagefield"
                        cq-msm-lockable="./urlRouter"
                        fieldLabel="URL"
                        name="./urlRouter"
                        rootPath="/content"/>
                </items>
            </column>
        </items>
    </content>
</jcr:root>
